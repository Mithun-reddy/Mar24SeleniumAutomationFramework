<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtf">
<suite name="Regression test">

	<test name="LoginOnChrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.HomePageTest"></class>
			<class name="tests.LoginTest">
				<methods>
					<exclude name="test2" />
				</methods>
			</class>
		</classes>
	</test>


	<test name="LoginOnSafari">
		<parameter name="browser" value="safari"></parameter>
		<classes>
			<class name="tests.HomePageTest"></class>
			<class name="tests.LoginTest">
				<methods>
					<exclude name="test2" />
				</methods>
			</class>
		</classes>
	</test>
</suite>

